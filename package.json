{
  "name": "com.appalachia.unity3d.ci.json-editor",
  "displayName": "Json Editor for Unity3D",
  "version": "0.1.0",
  "unity": "2020.3",
  "description": "JSON Editor drawer for Unity3D text assets.",
  "keywords": [
    "Scripts",
    "Tool"
  ],
  "category": "Tool",
  "license": "SEE LICENSE IN LICENSE.MD",
